  "scripts": {
    "cra-build": "react-scripts build",
    "cra-eject": "react-scripts eject",
    "cra-start": "react-scripts start",
    "cra-test": "react-scripts test",
    "dev": "nodemon --config config/nodemon/dev.json",
    "dev:nowatch": "cross-env NODE_ENV=development BABEL_ENV=development webpack-dev-server --config config/webpack/webpack.client.babel.ts",
    "dev:clean": "rimraf '../.build/development/{*,.*}' 'node_modules/.cache'",
    "prod": "cross-env NODE_ENV=production BABEL_ENV=production webpack --config config/webpack/webpack.client.babel.ts",
    "prod:clean": "rimraf '../.build/production/{*,.*}' 'node_modules/.cache'",
    "prod:serve:nowatch": "babel-node --extensions \".ts\" ./src/server/server.ts",
    "prod:serve": "nodemon --config config/nodemon/serve.json",
    "prod:watch": "nodemon --config config/nodemon/prod.json",
    "lint": "yarn eslint; yarn stylelint; yarn tsc; yarn tslint",
    "lint:fix": "yarn eslint:fix; yarn tsc; yarn tslint:fix",
    "lint:watch": "nodemon --config config/nodemon/lint.json",
    "lint:fix:watch": "nodemon --config config/nodemon/lint.fix.json",
    "eslint": "eslint --report-unused-disable-directives --format codeframe '**/*.{js,jsx,ts,tsx,json}'",
    "eslint:fix": "yarn eslint --fix",
    "eslint:watch": "nodemon --config config/nodemon/eslint.json",
    "eslint:fix:watch": "nodemon --config config/nodemon/eslint.fix.json",
    "tsc": "tsc --project tsconfig.json --noEmit",
    "tsc:watch": "yarn tsc --watch",
    "tslint": "tslint --project tsconfig.json --config tslint.json --format codeFrame",
    "tslint:watch": "nodemon --config config/nodemon/tslint.json",
    "tslint:fix": "yarn tslint --fix",
    "tslint:fix:watch": "nodemon --config config/nodemon/tslint.fix.json",
    "stylelint": "stylelint '**/*.*(css|scss)'",
    "stylelint:fix": "yarn stylelint --fix",
    "stylelint:watch": "nodemon --config config/nodemon/stylelint.json",
    "stylelint:fix:watch": "nodemon --config config/nodemon/stylelint.fix.json",
    "analyze": "cross-env ANALYZE=1 yarn prod",
    "analyze:clean": "rimraf ../.build/analyze/* node_modules/.cache",
    "analyze:watch": "nodemon --config config/nodemon/analyze.json",
    "format": "prettier --check '**/*.{js,jsx,ts,tsx,json,html,css,less,scss,yml,yaml}'",
    "format:fix": "yarn format --write",
    "test": "jest --config=config/jest/jest.config.js",
    "test:lint": "WITH_ESLINT=1 yarn test",
    "test:watch": "yarn test --watch --verbose",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --config=config/jest/jest.config.js --runInBand --watch",
    "test:coverage": "yarn test --coverage",
    "e2e": "cypress run",
    "e2e:dev": "cypress open"
  },
  "eslintConfig": {
    "extends": "react-app"
